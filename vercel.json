{
    "headers": [{
        "source": "/(.*)",
        "headers": [{
                "key": "Content-Security-Policy",
                "value": "default-src 'self';img-src *;connect-src https://*;frame-ancestors 'none';script-src 'self';frame-src 'none'; font-src 'self' https://cdnjs.cloudflare.com; media-src 'none'; object-src 'none'; manifest-src 'self'; worker-src 'self'; prefetch-src 'self'"
            },
            {
                "key": "X-Content-Type-Options",
                "value": "nosniff"
            },
            {
                "key": "X-Frame-Options",
                "value": "DENY"
            },
            {
                "key": "X-XSS-Protection",
                "value": "1; mode=block"
            },
            {
                "key": "Referrer-Policy",
                "value": "strict-origin"
            },
            {
                "key": "Permissions-Policy",
                "value": "geolocation=(self), microphone=()"
            }
        ]
    }]
}